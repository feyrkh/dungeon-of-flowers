[application]

config/name="Dungeon of Flowers"
run/main_scene="res://menu/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

EventBus="*res://EventBus.gd"
EnemyList="*res://combat/EnemyList.gd"
MoveList="*res://combat/MoveList.gd"
CombatMgr="*res://combat/CombatMgr.tscn"
GameData="*res://GameData.gd"
MusicCrossFade="*res://util/MusicCrossFade.tscn"
AudioPlayerPool="*res://util/AudioPlayerPool.tscn"
DialogicSingleton="*res://addons/dialogic/Other/DialogicSingleton.gd"
QuestMgr="*res://QuestMgr.gd"

[debug]

settings/fps/force_fps=60
settings/stdout/print_fps=true
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogic/plugin.cfg" )

[global]

error=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
